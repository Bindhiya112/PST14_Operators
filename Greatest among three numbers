public class Main {
    public static void main(String[] args) {
        int a = 12;
        int b = 25;
        int c = 18;
        if (a >= b && a >= c) {
            System.out.println("Greatest number is: " + a);
        }
        else if (b >= a && b >= c) {
            System.out.println("Greatest number is: " + b);
        }
        else {
            System.out.println("Greatest number is: " + c);
        }
    }
}
